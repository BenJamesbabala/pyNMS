    "import tkinter as tk\n",
    "\n",
    "# start the main tkinter window\n",
    "window = tk.Tk()\n",
    "# create a canvas\n",
    "canvas = tk.Canvas(window, background='white')\n",
    "# display the canvas\n",
    "canvas.pack()\n",
    "# run the application\n",
    "window.mainloop()"
   ]
